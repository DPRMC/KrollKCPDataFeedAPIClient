{
    "name": "dprmc/kroll-kcp-data-feed-api-client",
    "description": "A PHP client library to interface with the Kroll KCP Data Feed API.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "michaeldrennen",
            "email": "michaeldrennen74@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-simplexml": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "guzzlehttp/guzzle": "^6|^7|^8",
        "nesbot/carbon": "^2|^3"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2",
        "phpunit/phpunit": "^9|^10|^11"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "DPRMC\\KrollKCPDataFeedAPIClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Tests\\": "tests"
        }
    }
}
